{
  "name": "express-api-starter",
  "version": "2.0.0",
  "description": " A basic starter for an express.js API",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "prestart": "npm run build",
    "build": "rimraf dist && babel src --copy-files --out-dir dist --source-maps",
    "start:dev": "nodemon --watch .env --watch src --exec babel-node src --source-maps",
    "test": "mocha --exit",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "knex": "babel-node node_modules/.bin/knex",
    "knex:seed": "yarn knex seed:run --knexfile src/knexfile.js",
    "knex:migrate": "yarn knex migrate:latest --knexfile src/knexfile.js",
    "knex:rollback": "yarn knex migrate:rollback --knexfile src/knexfile.js",
    "knex:prettier": "prettier --write **/*.{js,json,yml,yaml,md}",
    "knex:make:seeder": "yarn knex seed:make --knexfile src/knexfile.js",
    "knex:make:migration": "yarn knex migrate:make --knexfile src/knexfile.js",
    "seed": "sequelize db:seed",
    "migrate": "sequelize db:migrate",
    "rollback:migration": "sequelize db:migrate:undo",
    "rollback:seed": "sequelize db:seed:undo",
    "make:seeder": "sequelize seed:create",
    "make:migration": "sequelize migration:create",
    "scaffold:entity": "node -e 'require(\"./src/scaffolders/generate.js\").g()'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,yml,yml,json}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "javascript",
    "node",
    "express",
    "es6",
    "api"
  ],
  "author": "amn satija. <satija.aman@gmail.com> (https://amansatija.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/w3cj/express-api-starter.git"
  },
  "license": "MIT",
